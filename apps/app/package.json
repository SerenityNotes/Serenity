    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@serenity-tools/editor": "*",
    "@serenity-tools/libsodium": "*",
    "@serenity-tools/ui": "*",
    "expo": "~44.0.0",
    "expo-asset": "~8.4.4",
    "expo-constants": "~13.0.0",
    "expo-dev-client": "~0.8.4",
    "expo-font": "~10.0.4",
