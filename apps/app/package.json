{
  "name": "serenity",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "yarn && expo start --dev-client",
    "dev:android": "yarn && cd ../../packages/editor && yarn build && cd - && expo start --dev-client --android",
    "dev:ios": "yarn && cd ../../packages/editor && yarn build && cd - && expo start --dev-client --ios",
    "dev:web": "yarn && yarn dev --web --dev-client",
    "dev:electron-web": "yarn && EXPO_ELECTRON_MODE=true expo start --web --dev-client",
    "dev:electron-main": "yarn && electron ./electron/main/index.ts",
    "build:web": "expo build:web --no-pwa",
    "eject": "expo eject",
    "test": "echo TODO",
    "ts:check": "yarn tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@mobily/stacks": "^2.0.0",
