{
  "cli": {
    "version": ">= 0.48.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "SERENITY_ENV": "development"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "SERENITY_ENV": "development"
      }
    },
    "preview-staging": {
      "distribution": "internal",
      "env": {
        "SERENITY_ENV": "staging"
      }
    },
    "preview-production": {
      "distribution": "internal",
      "env": {
        "SERENITY_ENV": "production"
      }
    },
    "production": {
      "env": {
        "SERENITY_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
